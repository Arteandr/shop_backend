{
    "swagger": "2.0",
    "info": {
        "description": "API server",
        "title": "FinlandShop API",
        "contact": {},
        "version": "0.1"
    },
    "host": "localhost:8000",
    "basePath": "/api/v1",
    "paths": {
        "/users/auth/sign-up": {
            "post": {
                "description": "Users sign-up",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "users-auth"
                ],
                "summary": "Sign-up",
                "parameters": [
                    {
                        "description": "sign-up input",
                        "name": "input",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/v1.signUpInput"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "ok",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "v1.signUpInput": {
            "type": "object",
            "required": [
                "email",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string",
                    "maxLength": 255
                },
                "password": {
                    "type": "string",
                    "maxLength": 64,
                    "minLength": 6
                }
            }
        }
    }
